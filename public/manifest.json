{
  "manifest_version": 2,
  "name": "Bills & Stuff",
  "description": "Makes it easier to track your bills and stuff.",
  "version": "0.0.1",
  "icons": {
    "128": "chrome-icon-lg.png"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "bookmarks",
    "cookies"
  ],
  "browser_action": {
    "default_icon": "chrome-icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["assets/background.js"],
    "persistent": true
  },
  "options_page": "index.html?#/options"
}
